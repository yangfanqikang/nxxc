<view class="bet-info" bind:__l="__l"><view class="previousNo" redBall="{{redBall}}" blueBall="{{blueBall}}">上期开奖：<text class="redClass">{{redBall}}</text><text class="blueClass">{{blueBall}}</text></view><view class="{{['award-table',(hide)?'hideClass':'']}}"><view class="award-header"><view class="award-tr"><view class="award-td">期号</view><view class="award-td">开奖号码</view></view></view><view class="awardBody"><block wx:for="{{aitem}}" wx:for-item="aitem" wx:for-index="index" wx:key="index"><view class="award-tr"><view class="award-td">{{"第"+aitem.term_no+"期"}}</view><view class="award-td">{{aitem.result}}</view></view></block></view></view><view data-event-opts="{{[['tap',[['toggle',['$event']]]]]}}" class="toggleButton" bindtap="__e"><image src="../../../../static/icon-dropdown.png"></image></view><view class="info-bottom"><view class="current-info"><image src="../static/message.png" mode></image><text current="{{current}}">{{current+"期截止时间："}}</text><text class="redClass" currentDate="{{currentDate}}">{{currentDate}}</text></view><view class="btn-group"><button data-event-opts="{{[['tap',[['random01',['$event']]]]]}}" bindtap="__e">机选1注</button><button disabled="{{tooLarge}}" data-event-opts="{{[['tap',[['random05',['$event']]]]]}}" bindtap="__e">机选5注</button><view><view mount="{{mount}}">{{"共 "+mount+" 注"}}</view><view total="{{total}}">{{"共 "+total+" 元"}}</view></view><button data-event-opts="{{[['tap',[['btnSure',['$event']]]]]}}" bindtap="__e">确定</button></view></view></view>