.prizeInfo {
  padding: 24rpx;
}
.prizeInfo > view:nth-child(1) {
    display: -webkit-box;
    display: -webkit-flex;
    display: -ms-flexbox;
    display: flex;
    -webkit-box-pack: justify;
    -webkit-justify-content: space-between;
        -ms-flex-pack: justify;
            justify-content: space-between;
    color: #666;
    font-size: 32rpx;
    margin-bottom: 36rpx;
}
.prizeInfo > view:nth-child(1) > text:last-child {
      color: #999;
}
.prizeInfo .prizeBall {
    display: -webkit-box;
    display: -webkit-flex;
    display: -ms-flexbox;
    display: flex;
    margin-bottom: 12rpx;
}
.prizeInfo .prizeBall > view {
      width: 54rpx;
      height: 54rpx;
      border-radius: 50%;
      margin-right: 20rpx;
      line-height: 54rpx;
      text-align: center;
      border: 1px solid #E73F3F;
      color: #E73F3F;
}
.prizeMoney {
  -webkit-box-sizing: border-box;
          box-sizing: border-box;
  padding: 24rpx;
  background-color: #f2f3f9;
  display: -webkit-box;
  display: -webkit-flex;
  display: -ms-flexbox;
  display: flex;
  -webkit-box-pack: justify;
  -webkit-justify-content: space-between;
      -ms-flex-pack: justify;
          justify-content: space-between;
}
.prizeMoney > view {
    width: 50%;
}
.prizeMoney > view > view {
      padding-left: 56rpx;
      background: url(data:image/png;base64,);
      background-repeat: no-repeat;
      background-position: left center;
      background-size: 42rpx 42rpx;
}
.prizeMoney > view > text {
      font-size: 42rpx;
      color: #DC504E;
}
.tabHeader {
  width: 100%;
  background-color: #DC504E;
  height: 64rpx;
  line-height: 64rpx;
  text-align: center;
  font-size: 32rpx;
  color: #fff;
}
.prizeSet .tr {
  display: -webkit-box;
  display: -webkit-flex;
  display: -ms-flexbox;
  display: flex;
}
.prizeSet .tr:nth-child(2n) {
    background-color: #f2f3f9;
}
.prizeSet .tr > .td {
    text-align: center;
    height: 80rpx;
    line-height: 80rpx;
}
.prizeSet .tr > .td:nth-child(1) {
    width: 25%;
}
.prizeSet .tr > .td:nth-child(2) {
    width: 50%;
}
.prizeSet .tr > .td:nth-child(3) {
    width: 25%;
}
.prizeSet .theader > .tr {
  background-color: #f2f3f9;
  font-size: 32rpx;
  color: #333;
}

